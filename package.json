{
  "name": "teachstarter-assessment-vue",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --host 127.0.0.1 --port 8088",
    "build": "vue-cli-service build && cp dist/index.html dist/200.html && cp .htaccess dist/.htaccess",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.6.4",
    "dayjs": "^1.8.16",
    "nanoid": "^2.1.11",
    "vue": "^2.6.10",
    "vue-flatpickr-component": "^8.1.5",
    "vue-json-to-csv": "^1.1.8",
    "vue-router": "^3.1.2",
    "vue-scroll-sync": "^1.0.5",
    "vue-shortkey": "^3.1.7",
    "vue-smoothscroll": "^0.2.0",
    "vue-textarea-autosize": "^1.0.4",
    "vue-tour": "^1.2.0",
    "vue2-dragula-fixed": "^2.5.5",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.1",
    "@vue/cli-plugin-eslint": "~4.3.1",
    "@vue/cli-service": "~4.3.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.3.1",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-use-v-if-with-v-for": "off",
      "vue/require-v-for-key": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "resolutions": {
    "svgo": "1.3.0"
  }
}
